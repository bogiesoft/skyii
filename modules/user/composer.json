{
    "name": "foqat/yii2-admin",
    "description": "RBAC Auth manager for Yii2 ",
    "keywords": ["yii", "admin", "auth", "rbac"],
    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Pankaj Sanam",
            "email": "pankaj.sanam@aryavratinfotech.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.9"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "~2.0"
    },
    "suggest": {
        "yiisoft/yii2-bootstrap": "Used when using layout 'left-menu', 'right-menu' or 'top-menu'"
    },
    "autoload": {
        "psr-4": {
            "modules\\user\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
